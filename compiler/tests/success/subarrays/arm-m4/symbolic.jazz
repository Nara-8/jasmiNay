// checks that the symbolic analysis is able to detect that copies
// preserve the value of the index

param int N = 10;

export fn main (reg u32 i) -> reg u32 {
  stack u32[N] s;
  reg ptr u32[N] rs;
  reg ptr u32[1] r;
  reg u32 i2;
  reg u32 tmp res;
  inline int j;

  tmp = 0;
  for j = 0 to N {
    s[j] = tmp;
  }

  r = s[i:1];
  () = #spill(i); // STR
  () = #unspill(i); // LDR
  i2 = i; // MOV
  s[i2:1] = r; // i2 is detected as equal to i

  rs = s;
  res = rs[i];

  return res;
}
